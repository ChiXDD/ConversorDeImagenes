extends plantilla

block content

    .contenedor-principal
      h1 #{titulo}
      h2 Bienvenido al conversor a WEBP
      form(action="http://localhost:3000/convertirWEBP" enctype="multipart/form-data" method="post" class='form-subir')
        input(type="file" name="conWEBP" multiple="true" class='btn-subir' required)
        input(type="submit" value="Convertir a WEBP" class='btn-mandar')